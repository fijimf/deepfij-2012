<deepfij>
    <schedule name="NCAA 2012-2013" key="ncaa2013" status="active" primary="true" startup="cold">
        <conferences>
            <data role="initializer" class="com.fijimf.deepfij.workflow.datasource.NcaaComConferenceSource"/>
            <data role="exporter" class="com.fijimf.deepfij.data.exporter.ConferenceExporter">
                <parameter key="fileName" value="ncaa2013-conference-data.txt"/>
                <execution schedule="0 0,30 * * * ?"/>
            </data>
        </conferences>
        <aliases>
            <data role="initializer" class="com.fijimf.deepfij.workflow.datasource.TextAliasSource">
                <parameter key="resource" value="kenpom.alias.txt"/>
            </data>
            <data role="exporter" class="com.fijimf.deepfij.data.exporter.AliasExporter">
                <parameter key="fileName" value="ncaa2013-alias-data.txt"/>
                <execution schedule="0 0,30 * * * ?"/>
            </data>
        </aliases>
        <teams>
            <data role="initializer" class="com.fijimf.deepfij.workflow.datasource.NcaaComTeamSource"/>
            <data role="exporter" class="com.fijimf.deepfij.data.exporter.TeamExporter">
                <parameter key="fileName" value="ncaa2013-alias-data.txt"/>
                <execution schedule="0 0/10 * * * ?"/>
            </data>
        </teams>
        <games>
            <data role="initializer" class="com.fijimf.deepfij.workflow.datasource.NcaaComGameSource">
                <parameter key="startDate" value="20121109"/>
                <parameter key="endDate" value="20130415"/>
            </data>
            <data role="updater" class="com.fijimf.deepfij.workflow.datasource.NcaaComGameSource">
                <parameter key="startDate" value="20121109"/>
                <parameter key="endDate" value="20130415"/>
                <execution schedule="0 0 4,6 * * ?"/>
            </data>
            <reader role="verifier" class="com.fijimf.deepfij.workflow.datasource.NcaaComResultSource">
                <parameter key="startDate" value="20121109"/>
                <parameter key="endDate" value="20130415"/>
                <parameter key="notify" value="fijimf@gmail.com"/>
                <execution schedule="0 0 8 * * ?"/>
            </reader>
        </games>
        <results>
            <reader role="initializer" class="com.fijimf.deepfij.workflow.datasource.NcaaComResultSource">
                <parameter key="startDate" value="20121109"/>
                <parameter key="endDate" value="20130415"/>
            </reader>
            <reader role="updater" class="com.fijimf.deepfij.workflow.datasource.NcaaComResultSource">
                <parameter key="startDate" value="20121109"/>
                <parameter key="endDate" value="20130415"/>
                <execution schedule="0 30 4,6 * * ?"/>
            </reader>
            <reader role="verifier" class="com.fijimf.deepfij.workflow.datasource.NcaaComResultSource">
                <parameter key="startDate" value="20121109"/>
                <parameter key="endDate" value="20130415"/>
                <parameter key="notify" value="fijimf@gmail.com"/>
                <execution schedule="0 0 8 * * ?"/>
            </reader>
        </results>
    </schedule>
